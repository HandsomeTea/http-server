{
  "name": "http-server",
  "version": "1.0.0",
  "description": "common http server, include websocket.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./index.ts",
    "debug": "./node_modules/.bin/ts-node-dev --respawn --transpile-only --debug ./index.ts",
    "build": "./node_modules/.bin/tsc",
    "upgrade": "./node_modules/.bin/npm-check -u",
    "doc": "./node_modules/.bin/apidoc -i ./src -o ./public/doc",
    "pkg": "./node_modules/.bin/pkg --out-path ./build .",
    "eslint": "./node_modules/.bin/eslint ./ --fix",
    "test": "./node_modules/.bin/cross-env TS_NODE_PROJECT='test/tsconfig.test.json' ./node_modules/.bin/mocha test/index.ts",
    "cover-test": "./node_modules/.bin/nyc npm run test",
    "husky-init": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/personal-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/personal-server/issues"
  },
  "homepage": "https://github.com/HandsomeTea/personal-server#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.11.2",
    "agentkeepalive": "^4.2.1",
    "ali-sms": "^1.0.3",
    "amqp-connection-manager": "^4.1.7",
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-http-context": "^1.2.4",
    "ioredis": "^5.2.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^2.3.3",
    "log4js": "^6.7.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.7.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "sequelize": "^6.29.0",
    "xml-crypto": "^2.1.4",
    "xml-encryption": "^2.0.0",
    "xmlbuilder": "^15.1.1",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/ioredis": "^4.28.10",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/ldapjs": "^2.2.4",
    "@types/mocha": "^10.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/nodemailer": "^6.4.6",
    "@types/pinyin": "^2.10.0",
    "@types/supertest": "^2.0.12",
    "@types/xml-crypto": "^1.4.2",
    "@types/xml-encryption": "^1.2.1",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "apidoc": "^0.53.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "mocha": "^10.1.0",
    "npm-check": "^6.0.1",
    "nyc": "^15.1.0",
    "pkg": "^5.8.0",
    "siege": "^0.2.0",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "bin": "build/index.js",
  "pkg": {
    "scripts": [],
    "assets": [],
    "targets": [],
    "options": [
      "max_old_space_size=2560",
      "max_semi_space_size=512"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "ui": "bdd"
  }
}
