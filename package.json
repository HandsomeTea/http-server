{
  "name": "http-server",
  "version": "1.0.0",
  "description": "common web server",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./index.ts",
    "debug": "./node_modules/.bin/ts-node-dev --respawn --transpile-only --debug ./index.ts",
    "debug-brk": "node --inspect-brk ./index.js",
    "build": "./node_modules/.bin/tsc",
    "eslint": "./node_modules/.bin/eslint ./ --fix",
    "check": "npm run fix",
    "upgrade": "./node_modules/.bin/npm-check -u",
    "test": "./node_modules/.bin/mocha",
    "doc": "./node_modules/.bin/apidoc -i ./src -o ./public/doc",
    "testunit": "node -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/personal-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/personal-server/issues"
  },
  "homepage": "https://github.com/HandsomeTea/personal-server#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "agentkeepalive": "^4.1.3",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-http-context": "^1.2.4",
    "got": "^11.8.1",
    "ioredis": "^4.17.3",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.3",
    "underscore": "^1.11.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/ioredis": "^4.17.10",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/underscore": "^1.10.24",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "apidoc": "^0.25.0",
    "eslint": "^7.8.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.1.3",
    "npm-check": "^5.9.2",
    "siege": "^0.2.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "bin": "index.js",
  "pkg": {
    "scripts": [],
    "assets": [
      "./k8s/application.yaml"
    ],
    "targets": [
      "node12-linux-x64"
    ],
    "options": [
      "max_old_space_size=2560",
      "max_semi_space_size=512"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run fix"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run fix"
    ]
  }
}
