{
  "name": "http-server",
  "version": "1.0.0",
  "description": "common http server, include websocket.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./index.ts",
    "debug": "./node_modules/.bin/ts-node-dev --respawn --transpile-only --debug ./index.ts",
    "build": "./node_modules/.bin/tsc",
    "upgrade": "./node_modules/.bin/npm-check -u",
    "doc": "./node_modules/.bin/apidoc -i ./src -o ./public/doc",
    "pkg": "./node_modules/.bin/pkg --out-path ./build .",
    "eslint": "./node_modules/.bin/eslint ./ --fix",
    "test": "./node_modules/.bin/mocha",
    "husky-init": "husky install",
    "testunit": "node -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/personal-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/personal-server/issues"
  },
  "homepage": "https://github.com/HandsomeTea/personal-server#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "agentkeepalive": "^4.1.4",
    "ali-sms": "^1.0.3",
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "express-http-context": "^1.2.4",
    "got": "^11.8.2",
    "ioredis": "^4.28.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.0.12",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3-rc.0",
    "nodemailer": "^6.7.0",
    "sequelize": "^6.9.0",
    "underscore": "^1.13.1",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/ioredis": "^4.28.1",
    "@types/js-yaml": "^4.0.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/module-alias": "^2.0.1",
    "@types/mysql": "^2.15.19",
    "@types/nodemailer": "^6.4.4",
    "@types/underscore": "^1.11.3",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "apidoc": "^0.50.1",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "npm-check": "^5.9.2",
    "pkg": "^5.4.1",
    "siege": "^0.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "bin": "build/index.js",
  "pkg": {
    "scripts": [],
    "assets": [
      "./k8s/application.yaml"
    ],
    "targets": [
      "node12-linux-x64"
    ],
    "options": [
      "max_old_space_size=2560",
      "max_semi_space_size=512"
    ]
  }
}
